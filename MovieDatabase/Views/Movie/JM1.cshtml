@model IEnumerable<MovieDatabase.Models.Movie>

<H1>
   A List of How many Actors are born by State in the United States
</H1>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ActorBirthState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActorsPerState)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.ActorBirthState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActorsPerState)
        </td>
    </tr>
}

</table>
